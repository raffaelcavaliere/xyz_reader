<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android" android:transitionOrdering="sequential">
    <transitionSet>
        <changeImageTransform
            android:duration="400"
            android:interpolator="@android:interpolator/decelerate_cubic"></changeImageTransform>
        <changeBounds
            android:duration="400"
            android:interpolator="@android:interpolator/decelerate_cubic"></changeBounds>
        <changeTransform
            android:duration="400"
            android:interpolator="@android:interpolator/decelerate_cubic"></changeTransform>
        <changeClipBounds
            android:duration="400"
            android:interpolator="@android:interpolator/decelerate_cubic"></changeClipBounds>
        <targets>
            <target android:targetId="@id/thumbnail"></target>
            <target android:targetId="@id/photo"></target>
        </targets>
    </transitionSet>
</transitionSet>